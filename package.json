{
	"name": "my-personal-portfolio",
	"version": "0.1.0",
	"private": true,
	"homepage": "https://tiltedveggie.github.io/Alan-Aguilar",
	"dependencies": {
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^13.5.0",
		"jquery": "^3.6.0",
		"prettier": "^2.8.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^4.4.0",
		"react-router-dom": "^6.3.0",
		"react-scripts": "5.0.1",
		"react-scrollspy": "^3.4.3",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"eslint": "eslint .",
		"prepare": "husky install",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" --fix",
		"format": "prettier \"src/**/*.{js,jsx,ts,tsx,json}\" --write"
	},
	"lint-staged": {
		"src/**/*.{js,jsx,ts,tsx,json}": [
			"eslint --fix",
			"prettier --write"
		],
		"./*.{js,ts,json}": [
			"eslint --fix",
			"prettier --write"
		]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"eslint": "^8.38.0",
		"eslint-plugin-react": "^7.32.2",
		"gh-pages": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1"
	}
}
